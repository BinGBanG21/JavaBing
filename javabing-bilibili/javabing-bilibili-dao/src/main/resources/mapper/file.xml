<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD MAPPER 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.javabing.bilibili.dao.FileDao">

    <insert id="addFile" parameterType="com.javabing.bilibili.domain.File">
        insert into t_file(url,
                           `type`,
                           md5,
                           createTime) value ( #{url},
            #{type},
            #{md5},
            #{createTime}
            )
    </insert>

    <select id="getFileByMD5" parameterType="java.lang.String" resultType="com.javabing.bilibili.domain.File">
        select
            *
        from
            t_file
        where
            md5 = #{md5}
    </select>

</mapper>